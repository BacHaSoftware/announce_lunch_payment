#. module: bhs_announce_lunch_payment
#: model:mail.template,body_html:bhs_announce_lunch_payment.email_template_bhs_lunch_cron
msgid ""
"<table border=\"0\" cellpadding=\"0\" cellspacing=\"0\" style=\"padding-top: 16px; background-color: #F1F1F1; font-family:Verdana, Arial,sans-serif; color: #454748; width: 100%; border-collapse:separate;\">\n"
"                    <tr>\n"
"                        <td align=\"center\">\n"
"                            <table border=\"0\" cellpadding=\"0\" cellspacing=\"0\" width=\"590\" style=\"padding: 16px; background-color: white; color: #454748; border-collapse:separate;\">\n"
"\n"
"                                <tbody>\n"
"                                    <!-- HEADER -->\n"
"                                    <tr>\n"
"                                        <td align=\"center\" style=\"min-width: 590px;\">\n"
"                                            <table border=\"0\" cellpadding=\"0\" cellspacing=\"0\" width=\"590\" style=\"min-width: 590px; background-color: white; padding: 0px 8px 0px 8px; border-collapse:separate;\">\n"
"                                                <tr>\n"
"                                                    <td valign=\"middle\">\n"
"                                                        <span style=\"font-size: 10px;\">Lunch Order</span>\n"
"                                                        <br>\n"
"                                                    </td>\n"
"                                                    <td valign=\"middle\" align=\"right\" t-if=\"not user.company_id.uses_default_logo\">\n"
"                                                        <img t-attf-src=\"/logo.png?company={{ user.company_id.id }}\" style=\"padding: 0px; margin: 0px; height: auto; width: 80px;\" t-att-alt=\"user.company_id.name\">\n"
"                                                    </td>\n"
"                                                </tr>\n"
"                                                <tr>\n"
"                                                    <td colspan=\"2\" style=\"text-align:center;\">\n"
"                                                        <hr width=\"100%\" style=\"background-color:rgb(204,204,204);border:medium none;clear:both;display:block;font-size:0px;min-height:1px;line-height:0; margin:16px 0px 16px 0px;\">\n"
"                                                    </td>\n"
"                                                </tr>\n"
"                                            </table>\n"
"                                        </td>\n"
"                                    </tr>\n"
"                                    <!-- CONTENT -->\n"
"                                    <tr>\n"
"                                        <td align=\"center\" style=\"min-width: 590px;\">\n"
"                                            <table border=\"0\" cellpadding=\"0\" cellspacing=\"0\" width=\"590\" style=\"min-width: 590px; background-color: white; padding: 0px 8px 0px 8px; border-collapse:separate;\">\n"
"                                                <tr>\n"
"                                                    <td valign=\"top\" style=\"font-size: 13px;\">\n"
"                                                        <div>\n"
"                                                            <t t-set=\"order\" t-value=\"ctx.get('order')\"></t>\n"
"                                                            <t t-set=\"email_orders\" t-value=\"ctx.get('email_orders', [])\"></t>\n"
"                                                            <p>\n"
"                                                                Dear <t t-out=\"order.get('user_name')\"></t>,\n"
"                                                            </p>\n"
"                                                            <p>Here is summary of your lunch orders for period: <t t-out=\"order.get('start_date')\"></t> - <t t-out=\"order.get('end_date')\"></t></p>\n"
"                                                            <table>\n"
"                                                                <thead>\n"
"                                                                    <tr style=\"background-color:rgb(233,232,233);\">\n"
"                                                                        <th style=\"width: 100%; min-width: 96px; font-size: 13px;\">\n"
"                                                                            <strong>Date Order</strong>\n"
"                                                                        </th>\n"
"                                                                        <th style=\"width: 100%; min-width: 150px; font-size: 13px;\">\n"
"                                                                            <strong>Product</strong>\n"
"                                                                        </th>\n"
"                                                                        <th style=\"width: 100%; min-width: 96px; font-size: 13px;\" align=\"center\"><strong>Quantity</strong></th>\n"
"                                                                        <th style=\"width: 100%; min-width: 96px; font-size: 13px;\" align=\"center\">\n"
"                                                                            <strong>Price</strong>\n"
"                                                                        </th>\n"
"                                                                    </tr>\n"
"                                                                </thead>\n"
"                                                                <tbody>\n"
"                                                                    <t t-foreach=\"email_orders\" t-as=\"Item\">\n"
"                                                                        <tr>\n"
"                                                                             <td style=\"width: 100%; font-size: 13px;\" valign=\"top\" t-out=\"Item['date'] or ''\"></td>\n"
"                                                                            <td style=\"width: 100%; font-size: 13px;\" valign=\"top\" t-out=\"Item['product'] or ''\"></td>\n"
"                                                                            <td style=\"width: 100%; font-size: 13px;\" valign=\"top\" align=\"right\" t-out=\"Item['quantity'] or ''\"></td>\n"
"                                                                            <td style=\"width: 100%; font-size: 13px;\" align=\"right\" t-out=\"Item['price'] or ''\"></td>\n"
"                                                                        </tr>\n"
"                                                                    </t>\n"
"                                                                    <tr>\n"
"                                                                        <td></td>\n"
"                                                                        <td></td>\n"
"                                                                        <td style=\"width: 100%; font-size: 13px; border-top: 1px solid black;\">\n"
"                                                                            <strong>Total</strong>\n"
"                                                                        </td>\n"
"                                                                        <td style=\"width: 100%; font-size: 13px; border-top: 1px solid black;\" align=\"right\">\n"
"                                                                            <strong t-out=\"order.get('total_price')\"></strong>\n"
"                                                                        </td>\n"
"                                                                    </tr>\n"
"                                                                </tbody>\n"
"                                                            </table>\n"
"                                                            <p align=\"center\"><a t-att-href=\"ctx.get('dynamic_url')\">\n"
"                                                                <img t-att-src=\"ctx.get('dynamic_url')\" alt=\"Proceed with Payment\" style=\"height: 400px; width: auto;\"></a>\n"
"                                                            </p>\n"
"\n"
"                                                            <p>Thank you for your orders!</p>\n"
"                                                        </div>\n"
"\n"
"\n"
"                                                    </td>\n"
"                                                </tr>\n"
"                                                <tr>\n"
"                                                    <td style=\"text-align:center;\">\n"
"                                                        <hr width=\"100%\" style=\"background-color:rgb(204,204,204);border:medium none;clear:both;display:block;font-size:0px;min-height:1px;line-height:0; margin: 16px 0px 16px 0px;\">\n"
"                                                    </td>\n"
"                                                </tr>\n"
"                                            </table>\n"
"                                        </td>\n"
"                                    </tr>\n"
"\n"
"                                    <!-- FOOTER -->\n"
"                                    <tr>\n"
"                                        <td align=\"center\" style=\"min-width: 590px;\">\n"
"                                            <table border=\"0\" cellpadding=\"0\" cellspacing=\"0\" width=\"590\" style=\"min-width: 590px; background-color: white; font-size: 11px; padding: 0px 8px 0px 8px; border-collapse:separate;\">\n"
"                                                <tr>\n"
"                                                    <td valign=\"middle\" align=\"left\">\n"
"                                                        <t t-out=\"user.company_id.name or ''\">YourCompany</t>\n"
"                                                    </td>\n"
"                                                </tr>\n"
"                                                <tr>\n"
"                                                    <td valign=\"middle\" align=\"left\" style=\"opacity: 0.7;\">\n"
"                                                        <t t-out=\"user.company_id.phone or ''\">+1 650-123-4567</t>\n"
"                                                        <t t-if=\"user.company_id.phone and (user.company_id.email or user.company_id.website)\">\n"
"                                                            |\n"
"                                                        </t>\n"
"                                                        <t t-if=\"user.company_id.email\">\n"
"                                                            <a t-attf-href=\"'mailto:%s' % {{ user.company_id.email }}\" style=\"text-decoration:none; color: #454748;\" t-out=\"user.company_id.email or ''\">info@yourcompany.com\n"
"                                                            </a>\n"
"                                                        </t>\n"
"                                                        <t t-if=\"user.company_id.email and user.company_id.website\">|\n"
"                                                        </t>\n"
"                                                        <t t-if=\"user.company_id.website\">\n"
"                                                            <a t-attf-href=\"'%s' % {{ user.company_id.website }}\" style=\"text-decoration:none; color: #454748;\" t-out=\"user.company_id.website or ''\">\n"
"                                                                http://www.example.com\n"
"                                                            </a>\n"
"                                                        </t>\n"
"                                                    </td>\n"
"                                                </tr>\n"
"                                            </table>\n"
"                                        </td>\n"
"                                    </tr>\n"
"                                </tbody>\n"
"                            </table>\n"
"                        </td>\n"
"                    </tr>\n"
"                    <!-- POWERED BY -->\n"
"                    <tr>\n"
"                        <td align=\"center\" style=\"min-width: 590px;\">\n"
"                            <table border=\"0\" cellpadding=\"0\" cellspacing=\"0\" width=\"590\" style=\"min-width: 590px; background-color: #F1F1F1; color: #454748; padding: 8px; border-collapse:separate;\">\n"
"                                <tr>\n"
"                                    <td style=\"text-align: center; font-size: 13px;\">\n"
"                                        Powered by\n"
"                                        <a target=\"_blank\" href=\"https://www.odoo.com\" style=\"color: #875A7B;\">Odoo</a>\n"
"                                    </td>\n"
"                                </tr>\n"
"                            </table>\n"
"                        </td>\n"
"                    </tr>\n"
"                </table>\n"
"            "
msgstr ""

